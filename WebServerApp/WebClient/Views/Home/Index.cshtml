@{
    @using WebClient.Models;
    @model IEnumerable<UserRegistry>;

    <h1>Welcome to the Ubiquity Network</h1>;
    

    <div id="Users" hidden>
    <p>Current registered user IP and Port</p>        
    
        <table>

            @foreach(var user in Model)
            {
                <tr>
                    <td>@user.IPAddress</td>
                    <td>@user.Port</td>
                </tr>
            }
        </table>
    </div>
}
